const speller = {
  displayWord: function(arr){
    return arr.join("")
  }
}
const speller1 = Object.create(speller)

console.log(speller1.displayWord(['n', 'a', 't', 'a', 'l', 'i', 'e']))
 
 console.log(speller1.displayWord(['s', 'u','p','r','i','y', 'a']))

 console.log(speller1.displayWord(['t', 'a', 'b', 'b', 'y']))

 console.log(speller1.displayWord(['s', 'a', 'r', 'a', 'h']))
